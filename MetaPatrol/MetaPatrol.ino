void drawPlayer() {
  switch (player.mode) {
    case NORMAL:
      gb.display.drawImage(player.x, player.y-player.h+1,img_rover);
      break;
    case JUMP:
      gb.display.drawImage(player.x, player.y-player.h+1,img_rover);
