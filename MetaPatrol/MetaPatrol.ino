// ---------------------------------------------------------------------------
//    Initialisation du jeu
// ---------------------------------------------------------------------------
void initGame(){
  ref_ground = 58;
  timecount = 0;
  cooldown_regen = 50;
  timer_bloc_regen = 0;
  timer_regen_power = 0;
  for (byte i=0; i < NUM_PSHOOTS; i++) {
    p_shoot[i].state = INACTIVE;
    p_shoot[i].w = 4;
    p_shoot[i].h = 4;
  }
}
//---------------------------------------------------------------------------

// ---------------------------------------------------------------------------
//    Initialisation du joueur
// ---------------------------------------------------------------------------
void initPlayer(){
  player.w = 16;
  player.h = 12; 
  player.x = 0;
  player.xv = 0;
  player.y = ref_ground - player.h;
  player.yv = 0;
  player.mode = NORMAL;
  player.state = NORMAL;
  player.life = 2;
  player.power = 255;
  player.frame = 0;
  player.anim = false;
}
// ---------------------------------------------------------------------------

// ---------------------------------------------------------------------------
//    Initialisation du scrolling (sol, étoiles, fonds, etc.)
// ---------------------------------------------------------------------------
void initScrolling(){
  //Etoiles
  Xback[0] = 0;
  //Montagnes 1 - grises
  Xback[1] = 0;
  //Montagnes 2 - vertes
  Xback[2] = 0;
  //Sol
  bloc[0].w = gb.display.width() / (NUM_BLOCS-1) ;
  bloc[0].h = player.h/2;
  bloc[0].x = 0;
  bloc[0].y = ref_ground;
  bloc[0].type = TYPE1;
  for (byte i=1 ; i < NUM_BLOCS ; i++) {
  bloc[i].w =  bloc[0].w;
  bloc[i].h = bloc[0].h;
  bloc[i].type = bloc[0].type;
  bloc[i].y = bloc[0].y;
  bloc[i].x = bloc[i-1].x + bloc[i-1].w;
  }
  for (byte i=0 ; i < NUM_PLANTS ; i++) {
    plant[i].w = 16;
    plant[i].h = 40;
    plant[i].x = gb.display.width()+random(0,gb.display.width());
    plant[i].y = ref_ground-plant[i].h;
    plant[i].type = INACTIVE;
  }
}
