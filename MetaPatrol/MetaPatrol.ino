// ---------------------------------------------------------------------------
//    Mise à jour du sol, des fonds et du ciel
// ---------------------------------------------------------------------------
void updateScrolling(){
   for (byte i=0; i<5;i++) {            //réinitialisation des scrolling des fonds
    if (Xback[i] <= -240) {Xback[i]=0;}
   }
   //Etoiles (gamespeed 5)
   if( timecount % gamespeed[5] == 0) {Xback[0] --;}
   //Montagnes grises (gamespeed 4)
   if( timecount % gamespeed[4] == 0) {Xback[1] --;}
   //Montagnes vertes (gamespeed 3)
   if( timecount % gamespeed[3] == 0) {Xback[2] --;}
   //Plantes et verdure (gamespeed 2)
   if( timecount % gamespeed[2] == 0) {
    for (byte t=0;t<NUM_PLANTS;t++) {
      if (plant[t].type != INACTIVE) {plant[t].x--;}
      if (plant[t].type == INACTIVE) {plant[t].type = random(0,5);}
      if (plant[t].x < -plant[t].w) {
        plant[t].type = INACTIVE;
        plant[t].x = gb.display.width()+random(0,gb.display.width());}
    }
   }
   //Sol (gamespeed 1 - le plus rapide)
   if( timecount % gamespeed[1] == 0) {
    for (byte t=0; t<NUM_BLOCS ; t++) {
      bloc[t].x --;
      if (bloc[t].x < - bloc[t].w) {             //régénération d'un bloc de sol aléatoire
        bloc[t].x = gb.display.width()-1;
        bloc[t].type = 1; }
    }
   }
}
