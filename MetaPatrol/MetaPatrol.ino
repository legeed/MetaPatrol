// ---------------------------------------------------------------------------
//    Mise à jour du joueur - position, score, vies, etc.
// ---------------------------------------------------------------------------
void updatePlayer(){
  player.state = NORMAL;
//Mouvement - les valeurs ont été définies de manière empirique
  if (player.y < ref_ground-player.h){
   player.mode = JUMP;
   player.yv += 0.5;
   player.yv *= 0.75;
   } else {
   player.mode = NORMAL;
   player.yv = 0;}
  player.xv = 0;
  if(gb.buttons.repeat(BUTTON_RIGHT, 1)){
   player.xv += 2;}              //à droite
  if(gb.buttons.repeat(BUTTON_LEFT,1)){
    player.xv -= 2;}              //à gauche
  player.x = min(max(0,player.x + player.xv),gb.display.width() - player.w); //borne la coordonnée X
  if(gb.buttons.pressed(BUTTON_B) && player.mode != JUMP) {                     //Saut
   player.mode = JUMP;
   player.yv -= 4.5;
   player.y -= 1;
  } 
  player.y = min(max(0, player.y + player.yv),ref_ground-player.h); //gravité, bornée
//Tir
  timer_shoot_player ++;
  if(gb.buttons.repeat(BUTTON_A,1) && timer_shoot_player >= cooldown_shoot_player) {   //cooldown ok et appui sur A
    for (byte i=0; i<NUM_PSHOOTS;i++) {       //cherche une balle "dispo" dans le tableau pour le tir horizontal
      if(p_shoot[i].state == INACTIVE) {      //si une de dispo, alors je la génère et sort de la boucle avec break
          p_shoot[i].state = ACTIVE;
          player.shoot = true;
          player.power --;
          timer_shoot_player = 0;
          p_shoot[i].x = player.x;
          p_shoot[i].y = player.y;
          p_shoot[i].xv = 3;
          p_shoot[i].yv = 0;
          p_shoot[i].frame = 5;
          break;
          }
     }
     for (byte i=0; i<NUM_PSHOOTS;i++) {       //cherche une balle "dispo" dans le tableau pour le tir vertical
      if(p_shoot[i].state == INACTIVE) {      //si une de dispo, alors je la génère et sort de la boucle avec break
          p_shoot[i].state = ACTIVE;
          player.shoot = true;
          player.power --;
          timer_shoot_player = 0;
          p_shoot[i].x = player.x+1;
          p_shoot[i].y = player.y;
          p_shoot[i].xv = 0;
          p_shoot[i].yv = 3;
          p_shoot[i].frame = 5;
          break;
          }
     }
    } else {player.shoot = false;}
//Collisions avec un bloc
  for (byte p=0; p < NUM_OBSTACLES ; p++) {
    /*switch (obstacle[p].type) {
     case ROCK:
      if(gb.collideRectRect(player.x, player.y, player.w, player.h,bloc[p].x,bloc[p].y,bloc[p].w,bloc[p].h)) {
        player.state = BLOCKED;
        player.y = min(max(8, player.y - player.yv),ref_ground);
        if(player.y > ref_ground) {player.x = min(max(0,player.x - player.xv - 1),gb.display.width()-player.w);}
        }
      break;
     case MINE:
      if(gb.collideRectRect(player.x, player.y, player.w, player.h,bloc[p].x,bloc[p].y,bloc[p].w,bloc[p].h)) {
        player.state = BLOCKED;
        player.x = min(max(0,player.x - player.xv - 1),gb.display.width()-player.w);
        }
      break;  
     case BONUS:
      if(gb.collideRectRect(player.x, player.y, player.w, player.h,bloc[p].x,bloc[p].y,bloc[p].w,bloc[p].h)) {
        player.power = min(player.power + 34,255);
        bloc[p].type = NORMAL;}
      break;
     case GUN:
      if(gb.collideRectRect(player.x, player.y, player.w, player.h,bloc[p].x,bloc[p].y,bloc[p].w,bloc[p].h)) {
        player.power = min(player.power + 34,255);
        bloc[p].type = NORMAL;}
      break;
    }*/
  }
//Blocage sur bord gauche
  if (player.x <= 0 && player.state == BLOCKED){player.state = HIT;}
//Application des effets d'une touche
  if(player.state == HIT) {player.power -= 50;}
//Regénération de power
  if(player.state == NORMAL) {timer_regen_power ++;} else {timer_regen_power = 0;}
  if(player.shoot) {timer_regen_power = 0;}
  if(timer_regen_power >= cooldown_regen_power) {flag_regen_power = true;} else {flag_regen_power = false;}
  if(flag_regen_power) {player.power = min(player.power + 1,255);}
//Perte d'une vie
  if (player.power < 0) {
    player.life --;
    player.state = DEAD;
  }
}
